<h1>Listing acc_photos</h1>

<table>
  <thead>
    <tr>
      <th>Account</th>
      <th>Photo</th>
      <th>Default</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @accounts_photos.each do |account_photo| %>
      <tr>
        <td><%= account_photo.account_id %></td>
        <td><%= account_photo.photo %></td>
        <td><%= account_photo.default %></td>
        <td><%= link_to 'Show', [:admin,account_photo] %></td>
        <td><%= link_to 'Edit', [:edit, :admin, account_photo] %></td>
        <td><%= link_to 'Destroy', [:admin, account_photo], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Acc photo', new_admin_account_photo_path %>
